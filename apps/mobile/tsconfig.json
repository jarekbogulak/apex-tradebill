{
  "extends": "@apex-tradebill/tsconfig/base.json",
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "bundler",
    "jsx": "react-native",
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "expo-router/paths": ["./types/expo-router-paths"]
    },
    "typeRoots": ["./node_modules/@types", "../../node_modules/@types"]
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts", "types/**/*.d.ts"],
  "references": []
}
